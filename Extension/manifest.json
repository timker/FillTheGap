{
    "manifest_version": 2,
    "name": "Fill The Gap ",
    "version": "1.0.2",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "eventPage.js"
        ]
    },
    "icons": {
        "128": "images/ftg128.png"
    },
    "web_accessible_resources": [
        "data.json",
        "template.html",
        "content.css"
    ]
}